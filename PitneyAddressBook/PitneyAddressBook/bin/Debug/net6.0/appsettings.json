{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      },
      "Using": []
    },

    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "BackupFolder/log.txt",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },

  "AllowedHosts": "*",
  "BackupFolder": {
    "AddressBook": "BackupFolder/AddressBook.json"
  }
}
